@model ModeloVeiculo
@{
    ViewData["Title"] = "Excluir Modelo de Veiculo";
}

<div class="text-center">

    <p class="display-6"> Deseja excluir modelo de veiculo @Model.NomeModelo ?</p>
    <br />
    <a type="button" class="btn btn-outline-danger" asp-controller="ModeloVeiculos" asp-action="Apagar" asp-route-id="@Model.Id">Excluir</a>
    <a type="button" class="btn btn-outline-secondary" asp-controller="ModeloVeiculos" asp-action="Index">Cancelar</a>
</div>
